{
  "rewrites": [
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    }
  ],
  "headers": [
    {
      "source": "/media/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}